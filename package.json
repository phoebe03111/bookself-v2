    "url": "https://github.com/phoebe03111/bookself-v2/issues"
  },
  "homepage": "https://github.com/phoebe03111/bookself-v2#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^16.1.4",
    "nodemon": "^2.0.22"
